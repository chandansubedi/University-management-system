{
    "version": 2,
    "builds": [
      {
        "src": "student_management_system/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "student_management_system/wsgi.py"
      }
    ]
  }